#include <stdio.h>

int main()

{  
   int i,n,max=0,min,a[100];
   printf("Enter size of array");
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
  min=a[0];
   for(i=0;i<n;i++)
   {
       if(a[i]>max)
       max=a[i];
   }
   for(i=0;i<n;i++)
   {
       if(a[i]<min)
       min=a[i];
   }
   printf("%d\t%d",max,min);
}
